{
    "name" : "AirBinNewTab",
    "version" : "1.1",
    "manifest_version" : 2,
    "description": "Open links to properties on AirBnB in new tabs",
    "content_scripts" : [
        {
            "matches": [
                        "https://www.airbnb.com/search*", 
                        "https://www.airbnb.co.uk/search*",
                        "https://www.airbnb.co.uk/s/*" ],
            "js" : [
		"src/jquery-1.7.1.min.js",
		"src/contentscript.js"]
        }
    ]
}
